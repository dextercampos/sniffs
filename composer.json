{
    "name": "natepage/sniffs",
    "description": "Package containing PHPCodeSniffer Sniffs.",
    "type": "package",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.3",
        "slevomat/coding-standard": "^4.8",
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": { "NatePage\\Sniffs\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "NatePage\\Sniffs\\": "tests/" }
    }
}
